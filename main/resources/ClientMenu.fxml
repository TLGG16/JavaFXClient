<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controllers.Client.Menu">
   <children>
      <Label layoutX="340.0" layoutY="26.0" text="Меню клиента">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="Orderbtn" layoutX="325.0" layoutY="79.0" mnemonicParsing="false" onAction="#Order_Pressed" prefHeight="60.0" prefWidth="150.0" text="Сделать заказ" textAlignment="CENTER" />
      <Button fx:id="Reviewbtn" layoutX="325.0" layoutY="153.0" mnemonicParsing="false" onAction="#Review_Pressed" prefHeight="60.0" prefWidth="150.0" text="Оставить отзыв&#10;на товар" textAlignment="CENTER" />
      <Button fx:id="Backbtn" layoutX="14.0" layoutY="538.0" mnemonicParsing="false" onAction="#Back_Pressed" prefHeight="48.0" prefWidth="117.0" text="Назад" textAlignment="CENTER" />
   </children>
</AnchorPane>
