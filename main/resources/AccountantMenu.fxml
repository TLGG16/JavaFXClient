<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controllers.Accountant.Menu">
   <children>
      <Label layoutX="328.0" layoutY="24.0" text="Меню бухгалтера">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="Reportbtn" layoutX="325.0" layoutY="79.0" mnemonicParsing="false" onAction="#Report_Pressed" prefHeight="60.0" prefWidth="150.0" text="Сделать отчет" textAlignment="CENTER" />
      <Button fx:id="FinalOrderViewbtn" layoutX="325.0" layoutY="153.0" mnemonicParsing="false" onAction="#FinalOrderView_Pressed" prefHeight="60.0" prefWidth="150.0" text="Просмотреть заказы" textAlignment="CENTER" />
      <Button fx:id="OrderViewbtn" layoutX="325.0" layoutY="229.0" mnemonicParsing="false" onAction="#OrderView_Pressed" prefHeight="60.0" prefWidth="150.0" text="Просмотреть проданные товары" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="Backbtn" layoutX="14.0" layoutY="538.0" mnemonicParsing="false" onAction="#Back_Pressed" prefHeight="48.0" prefWidth="117.0" text="Назад" textAlignment="CENTER" />
      <Button fx:id="DelPricebtn" layoutX="326.0" layoutY="300.0" mnemonicParsing="false" onAction="#DelPrice_Pressed" prefHeight="60.0" prefWidth="150.0" text="Изменить тариф доставки" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="OrderFilterDatebtn" layoutX="326.0" layoutY="378.0" mnemonicParsing="false" onAction="#OrderFilterDate_Pressed" prefHeight="60.0" prefWidth="150.0" text="Фильтрация заказов по дате" textAlignment="CENTER" wrapText="true" />
   </children>
</AnchorPane>
