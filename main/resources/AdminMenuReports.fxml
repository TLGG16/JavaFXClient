<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controllers.Admin.Reports.Menu">
   <children>
      <Label layoutX="269.0" layoutY="36.0" text="Меню для работы с отчетами">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="CreateReportbtn" layoutX="309.0" layoutY="100.0" mnemonicParsing="false" onAction="#CreateReport_Pressed" prefHeight="60.0" prefWidth="150.0" text="Добавить отчет" textAlignment="CENTER" />
      <Button fx:id="ShowReportsbtn" layoutX="309.0" layoutY="180.0" mnemonicParsing="false" onAction="#ShowReports_Pressed" prefHeight="60.0" prefWidth="150.0" text="Просмотреть очеты" textAlignment="CENTER" />
      <Button fx:id="DeleteReportbtn" layoutX="310.0" layoutY="339.0" mnemonicParsing="false" onAction="#DeleteReport_Pressed" prefHeight="60.0" prefWidth="150.0" text="Удалить отчет" textAlignment="CENTER" />
      <Button fx:id="UpdateReportbtn" layoutX="310.0" layoutY="259.0" mnemonicParsing="false" onAction="#UpdateReport_Pressed" prefHeight="60.0" prefWidth="150.0" text="Изменить отчеты" textAlignment="CENTER" />
      <Button fx:id="Backbtn" layoutX="14.0" layoutY="538.0" mnemonicParsing="false" onAction="#Back_Pressed" prefHeight="48.0" prefWidth="117.0" text="Назад" textAlignment="CENTER" />
   </children>
</AnchorPane>
